{
  "tags"     : {
    "allowUnknownTags": true
  },
  "source"   : {
    "include"       : ["./src/"],
    "includePattern": ".+[.]js?$",
    "excludePattern": "docs"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks"    : false,
    "monospaceLinks" : true,
    "systemName"     : "remoteFileExplorer",
    "footer"         : "Created by evanmao",
    "copyright"      : "2013",
    "navType"        : "vertical",
    "theme"          : "haruki",
    "linenums"       : true,
    "collapseSymbols": true,
    "inverseNav"     : false
  },
  "opts"     : {
    "encoding"   : "utf8",
    "destination": "./dist/docs/",
    "recurse"    : true,
    "private"    : true
  }
}